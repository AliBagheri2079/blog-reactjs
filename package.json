{
  "name": "story-reactjs",
  "homepage": "https://alibagheri2079.github.io/story-reactjs",
  "title": "Story Telling About Nothing",
  "description": "In this Webpage you see a lot of animation and tricks...",
  "author": "Ali Bagheri",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "vite build",
    "preview": "vite preview",
    "lint:scripts": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:scripts:fix": "yarn run lint:scripts --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint": "yarn run lint:scripts:fix && yarn run lint:styles",
    "format:scripts": "prettier . --write",
    "format:styles": "yarn run lint:styles --fix",
    "format": "yarn run format:scripts && yarn run format:styles",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@heroicons/react": "^2.0.17",
    "@material-tailwind/react": "^1.4.2",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.1",
    "circular-revealer": "^0.0.9",
    "classnames": "^2.3.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-particles": "^2.9.3",
    "tsparticles": "^2.9.3"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
